    <script>
        // Get references to DOM elements
        const lookupBtn = document.getElementById('lookup-btn');
        const primaryQueryInput = document.getElementById('primary-query');
        const phoneNumberInput = document.getElementById('phone-number');
        const addressLocationInput = document.getElementById('address-location');
        const imageUrlInput = document.getElementById('image-url');
        const localImageUploadInput = document.getElementById('local-image-upload');
        const imagePreview = document.getElementById('image-preview');
        const noImageText = document.getElementById('no-image-text');
        const resultsSection = document.getElementById('results-section');
        const systemLog = document.getElementById('system-log');

        // Initialize Tone.js Synth for sound effects
        const synth = new Tone.Synth({
            oscillator: { type: "sine" }, // A simple sine wave for a clean sound
            envelope: {
                attack: 0.005, // Very short attack
                decay: 0.1,   // Quick decay
                sustain: 0.0, // No sustain
                release: 0.1  // Short release
            }
        }).toDestination();

        // Function to play a sound
        function playScanSound() {
            synth.triggerAttackRelease("C5", "8n"); // Play C5 note for an 8th note duration
        }

