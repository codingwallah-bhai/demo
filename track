            let dorkString = '';
            biharDomains.forEach((domain, index) => {
                dorkString += `site:${domain}`;
                if (index < biharDomains.length - 1) {
                    dorkString += ' OR ';
                }
            });
            return dorkString;
        }

        // Global variable to store the Bihar Government dork string
        const BIHAR_GOVT_DORK = generateBiharGovtDorkString();

