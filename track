            ],
            "LEVEL 4: Technical & Government Records (तकनीकी और सरकारी रिकॉर्ड)": [
                { type: 'general', name: 'Shodan.io', desc: 'इंटरनेट से जुड़े डिवाइस (IoT) या कंपनी की संपत्तियां खोजें।', url: 'https://www.shodan.io/search?query=${query}' },
                { type: 'general', name: 'Indian Govt. Domains (Dork)', desc: 'सभी भारतीय सरकारी साइटों (.gov.in, .nic.in) पर खोजें।', url: 'https://www.google.com/search?q=site:.gov.in OR site:.nic.in "${query}"' },
                { type: 'general', name: 'Bihar Govt. Domains (Specific)', desc: 'बिहार सरकार की विशेष साइटों पर खोजें।', url: 'https://www.google.com/search?q=(${BIHAR_GOVT_DORK}) "${query}"' },
                { type: 'general', name: 'Indian Court Records (Dork)', desc: 'भारतीय कानूनी दस्तावेजों में खोजें।', url: 'https://www.google.com/search?q=site:indiankanoon.org OR site:casemine.com "${query}"' },
                { type: 'general', name: 'WHOIS Lookup (Dork)', desc: 'क्वेरी का उल्लेख करने वाले डोमेन पंजीकरण रिकॉर्ड खोजें।', url: 'https://www.google.com/search?q=inurl:"whois" "${query}"' },
                { type: 'general', name: 'Crypto Address Dork', desc: 'क्वेरी से जुड़े क्रिप्टोक्यूरेंसी पतों के लिंक खोजें।', url: 'https://www.google.com/search?q="${query}" "BTC Address" OR "ETH Address" OR "wallet"' },
                { type: 'general', name: 'Indian Election Commission (Dork)', desc: 'भारतीय चुनाव आयोग की साइटों पर सार्वजनिक चुनावी डेटा खोजें।', url: 'https://www.google.com/search?q=site:eci.gov.in OR site:ceodelhi.gov.in "${query}"' },
                { type: 'general', name: 'Govt. Employee Directory Dork', desc: 'सरकारी कर्मचारी निर्देशिकाओं में जानकारी खोजें।', url: 'https://www.google.com/search?q=site:.gov.in inurl:directory OR inurl:employees "${query}"' },
                { type: 'address', name: 'Google Maps Search (Address)', desc: 'Google Maps पर पता खोजें।', url: 'https://www.google.com/maps/search/${address}' },
                { type: 'general', name: 'Censys Search', desc: 'खुले पोर्ट और कॉन्फ़िगरेशन के लिए इंटरनेट पर डिवाइस, होस्ट और सर्टिफिकेट खोजें।', url: 'https://search.censys.io/search?query=${query}' },
                { type: 'general', name: 'ZoomEye Search', desc: 'IP एड्रेस, डोमेन और होस्टनाम पर आधारित साइबर स्पेस सर्च इंजन।', url: 'https://www.zoomeye.org/search?q=${query}' }
            ],
            "LEVEL 5: Google Ecosystem Deep Dive (Google इकोसिस्टम में गहराई से खोज)": [
                { type: 'general', name: 'Google Photos (Dork)', desc: 'सार्वजनिक Google Photos एल्बम/उल्लेख खोजें।', url: 'https://www.google.com/search?q=site:photos.google.com "${query}"' },
                { type: 'general', name: 'Google Drive (Dork)', desc: 'सार्वजनिक Google Drive/Docs फ़ाइलें खोजें।', url: 'https://www.google.com/search?q=site:drive.google.com OR site:docs.google.com "${query}"' },
                { type: 'general', name: 'Google Calendar (Dork)', desc: 'सार्वजनिक Google Calendar इवेंट खोजें।', url: 'https://www.google.com/search?q=site:calendar.google.com inurl:event "${query}"' },
                { type: 'general', name: 'Google Maps Reviews (Dork)', desc: 'Google Maps पर की गई समीक्षाएँ खोजें।', url: 'https://www.google.com/search?q=site:google.com/maps/contrib/ "${query}"' },
                { type: 'general', name: 'Google Groups (Dork)', desc: 'सार्वजनिक Google Groups चर्चाएँ खोजें।', url: 'https://www.google.com/search?q=site:groups.google.com "${query}"' },
                { type: 'general', name: 'Public Contact Files (Dork)', desc: 'इंटरनेट पर VCF/CSV संपर्क फ़ाइलें खोजें।', url: 'https://www.google.com/search?q=filetype:vcf OR filetype:csv intext:"${query}"'},
                { type: 'general', name: 'Google Spreadsheets Dork', desc: 'सार्वजनिक Google Sheets में संवेदनशील डेटा खोजें।', url: 'https://www.google.com/search?q=site:docs.google.com ext:xlsx OR ext:csv inurl:pub "${query}"' },
                { type: 'general', name: 'Google Alerts Creation (External)', desc: 'Google Alerts बनाने के लिए एक लिंक जो आपकी क्वेरी को ट्रैक करेगा।', url: 'https://www.google.com/alerts#${query}' }
            ],
            "LEVEL 6: Image-Based Reconnaissance (इमेज-आधारित टोही)": [
                { type: 'image', name: 'Google Reverse Image Search', desc: 'Google Images पर URL के माध्यम से इमेज खोजें।', url: 'https://images.google.com/searchbyimage?image_url=${image_url}' },
                { type: 'image', name: 'Yandex Reverse Image Search', desc: 'Yandex Images पर URL के माध्यम से इमेज खोजें (अक्सर Google से अलग परिणाम)।', url: 'https://yandex.com/images/search?url=${image_url}&rpt=imageview' },
                { type: 'image', name: 'TinEye Reverse Image Search', desc: 'TinEye पर URL के माध्यम से इमेज खोजें।', url: 'https://tineye.com/search?url=${image_url}' },
                { type: 'image', name: 'Bing Visual Search', desc: 'Bing पर URL के माध्यम से इमेज खोजें।', url: 'https://www.bing.com/images/search?q=imgurl:${image_url}&view=detailv2&iss=sbi' }
            ]
        };
           
        // Helper function for creating a delay (sleep)
        const sleep = ms => new Promise(resolve => setTimeout(resolve, ms));

        // Function to log messages to the system log with a typing animation
        async function logMessage(message, speed = 20) {
            const date = new Date();
            const timestamp = `[${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}:${date.getSeconds().toString().padStart(2, '0')}]`; // Format timestamp
            systemLog.innerHTML += `${timestamp} >> `; // Add timestamp
            for(let i = 0; i < message.length; i++) {
                systemLog.innerHTML += message[i]; // Add character by character
                systemLog.scrollTop = systemLog.scrollHeight; // Scroll to bottom
                await sleep(speed); // Pause for typing effect
            }
            systemLog.innerHTML += '\n'; // Add newline after message
            systemLog.scrollTop = systemLog.scrollHeight; // Ensure log stays scrolled to bottom
        }

        // Event listener for local image file selection
        localImageUploadInput.addEventListener('change', function(event) {
            const file = event.target.files[0];
            if (file) {
                const reader = new FileReader();
                reader.onload = function(e) {
                    imagePreview.src = e.target.result;
                    imagePreview.classList.remove('hidden');
                    noImageText.classList.add('hidden');
                    logMessage(`लोकल इमेज अपलोड की गई: "${file.name}"। (Local image uploaded: "${file.name}".)`, 15);
                    // The permanent help text below the input field already clarifies the public URL requirement
                };
                reader.readAsDataURL(file); // Read the file as a Data URL
            } else {
                imagePreview.src = "";
                imagePreview.classList.add('hidden');
                noImageText.classList.remove('hidden');
                logMessage("कोई लोकल इमेज नहीं चुनी गई। (No local image selected.)", 15);
            }
        });


        // Main function to execute the OSINT scan
        async function executeScan() {
            playScanSound(); // Play sound when scan starts
            lookupBtn.disabled = true; // Disable button during scan
            lookupBtn.innerText = "[ SCANNING... ]"; // Change button text
            resultsSection.innerHTML = ''; // Clear previous results
            resultsSection.classList.add('hidden'); // Hide results section initially
            systemLog.innerHTML = ''; // Clear system log

            const rawPrimaryQuery = primaryQueryInput.value.trim();
            const rawPhoneNumber = phoneNumberInput.value.trim();
            const rawAddressLocation = addressLocationInput.value.trim();
            const rawImageUrl = imageUrlInput.value.trim(); // URL from input box

            // Check if at least one input field is filled
            if (!rawPrimaryQuery && !rawPhoneNumber && !rawAddressLocation && !rawImageUrl) {
                await logMessage("CRITICAL_ERROR: कृपया कम से कम एक खोज क्वेरी दर्ज करें। (Please enter at least one search query.)", 10);
                lookupBtn.disabled = false;
                lookupBtn.innerText = "[ EXECUTE_SCAN ]";
                return;
            }
            
            await logMessage("Initializing HYDRA framework...");
            await sleep(200);
            await logMessage("Generating multi-vector attack surface...");
            await sleep(500);

            // Combine all text queries into a single 'query' for general dorks
            let combinedQuery = [];
            if (rawPrimaryQuery) combinedQuery.push(rawPrimaryQuery);
            if (rawPhoneNumber) combinedQuery.push(rawPhoneNumber);
            if (rawAddressLocation) combinedQuery.push(rawAddressLocation);
            const finalQuery = combinedQuery.join(' ').trim(); // Combine with space
            const encodedQuery = encodeURIComponent(finalQuery);
            // cleanQuery will be used for platforms that require very clean inputs (e.g., Telegram handles, Phonebook.cz)
            const cleanQuery = encodeURIComponent(finalQuery.replace(/[\+\s\-\(\)\/\\]/g, ''));

            // Encode individual inputs for specific links
            const encodedPhone = encodeURIComponent(rawPhoneNumber);
            const cleanPhone = encodeURIComponent(rawPhoneNumber.replace(/[\+\s\-\(\)]/g, ''));
            const encodedAddress = encodeURIComponent(rawAddressLocation);
            const cleanAddress = encodeURIComponent(rawAddressLocation.replace(/[\+\s\-\(\)\/\\]/g, ''));
            const encodedImageUrl = encodeURIComponent(rawImageUrl);

            // Iterate through each category of sources
            for (const category in sources) {
                const currentCategorySources = sources[category];
                // Filter sources based on input type and if the corresponding input has data
                const relevantSources = currentCategorySources.filter(source => {
                    if (source.type === 'general' && finalQuery) return true;
                    if (source.type === 'phone' && rawPhoneNumber) return true;
                    if (source.type === 'address' && rawAddressLocation) return true;
                    if (source.type === 'image' && rawImageUrl) return true; // Only if image URL is provided
                    return false;
                });

                if (relevantSources.length > 0) {
                    await logMessage(`${category} के लिए क्वेरी बना रहे हैं...`, 15);
                   
                    // Create category title element
                    const categoryTitle = document.createElement('h3');
                    categoryTitle.className = 'category-title text-xl font-bold mb-4 mt-6 pb-2 border-b-2';
                    categoryTitle.textContent = `// ${category}`;
                    resultsSection.appendChild(categoryTitle);

                    // Create a grid for links within the category
                    const grid = document.createElement('div');
                    grid.className = 'grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4';
                   
                    // Iterate through each relevant source within the current category
                    relevantSources.forEach(source => {
                        let finalUrl = source.url;
                        if (source.type === 'general') {
                            finalUrl = finalUrl.replace('${query}', encodedQuery);
                            finalUrl = finalUrl.replace('${clean_query}', cleanQuery);
                            // Handle specific dork placeholders if present
                            if (finalUrl.includes('${query_site}')) {
                                // Example: if user enters "site:example.com keyword"
                                const parts = finalQuery.split('site:');
                                if (parts.length > 1) {
                                    const siteAndKeyword = parts[1].split(' ');
                                    finalUrl = finalUrl.replace('${query_site}', encodeURIComponent(siteAndKeyword[0]));
                                    finalUrl = finalUrl.replace('${query_main}', encodeURIComponent(siteAndKeyword.slice(1).join(' ')));
                                } else {
                                    finalUrl = finalUrl.replace('${query_site}', '');
                                    finalUrl = finalUrl.replace('${query_main}', encodedQuery);
                                }
                            }
                        } else if (source.type === 'phone') {
                            finalUrl = finalUrl.replace('${phone}', encodedPhone);
                            finalUrl = finalUrl.replace('${clean_phone}', cleanPhone);
                        } else if (source.type === 'address') {
                            finalUrl = finalUrl.replace('${address}', encodedAddress);
                            finalUrl = finalUrl.replace('${clean_address}', cleanAddress);
                        } else if (source.type === 'image') {
                            finalUrl = finalUrl.replace('${image_url}', encodedImageUrl);
                        }
                       
                        // Create the link card element
                        const linkCard = document.createElement('a');
                        linkCard.href = finalUrl; // Set the generated URL
                        linkCard.target = '_blank'; // Open in new tab
                        linkCard.rel = 'noopener noreferrer'; // Security best practice
                        linkCard.className = 'block w-full link-card p-3 rounded-md';
                       
                        // Create name and description elements for the link card
                        const name = document.createElement('p');
                        name.className = 'font-bold link-name text-lg';
                        name.textContent = `[+] ${source.name}`;

                        const desc = document.createElement('p');
                        desc.className = 'text-sm link-desc mt-1';
                        desc.textContent = source.desc;

                        // Append elements to the link card and then to the grid
                        linkCard.appendChild(name);
                        linkCard.appendChild(desc);
                        grid.appendChild(linkCard);
                    });
                    resultsSection.appendChild(grid); // Add the grid of links to the results section
                    await sleep(250); // Small delay between categories
                }
            }

            await logMessage("सभी स्कैन वेक्टर उत्पन्न हो गए हैं। उपयोगकर्ता क्रिया का इंतजार है। (All scan vectors generated. Awaiting user action.)", 10);
            await logMessage("शुभ शिकार। (Happy hunting.)", 50);

            resultsSection.classList.remove('hidden'); // Show the results section
            lookupBtn.disabled = false; // Re-enable the button
            lookupBtn.innerText = "[ EXECUTE_SCAN ]"; // Reset button text
        }

        // Add event listeners
        lookupBtn.addEventListener('click', executeScan); // On button click
        // Add event listeners for Enter key on all input fields
        primaryQueryInput.addEventListener('keyup', (event) => {
            if (event.key === 'Enter') { executeScan(); }
        });
        phoneNumberInput.addEventListener('keyup', (event) => {
            if (event.key === 'Enter') { executeScan(); }
        });
        addressLocationInput.addEventListener('keyup', (event) => {
            if (event.key === 'Enter') { executeScan(); }
        });
        imageUrlInput.addEventListener('keyup', (event) => {
            if (event.key === 'Enter') { executeScan(); }
        });
    </script>
</body>
</html>
